#. This text is shown in the URL bar when it's empty
msgctxt "urlbar_hint"
msgid "Search or enter address"
msgstr "ຄົ້ນຫາ ຫລື ປ້ອນທີ່ຢູ່ໃສ່ນີ້"

msgctxt "teaser"
msgid ""
"Automatic private browsing.\n"
"Browse. Erase. Repeat."
msgstr ""
"ການທ່ອງເວັບສ່ວນຕົວແບບອັດຕະໂນມັດ.\n"
"ທ່ອງເວັບ ແລະ ລົບລ້າງປະຫວັດການໃຊ້ງານອອກ."

#. "Snackbar" shown after the user has pressed the 'erase' button at the end
#. of a browsing session
msgctxt "feedback_erase"
msgid "Your browsing history has been erased."
msgstr "ປະຫວັດການທ່ອງເວັບຂອງທ່ານໄດ້ຖືກລຶບອອກແລ້ວ."

#. This label is shown below the URL bar as soon as the user enters text.
#. Clicking the label will perform a search
#, c-format
msgctxt "search_hint"
msgid "Search for %1$s"
msgstr "ຄົ້ນຫາສຳລັບ %1$s"

#. Menu item: Navigate back (in browser)
msgctxt "menu_back"
msgid "Back"
msgstr "ກັບຄືນ"

#. Menu item: Navigate forward (in browser)
msgctxt "menu_forward"
msgid "Forward"
msgstr "ໄປຂ້າງຫນ້າ"

#. Menu item: Refresh / Reload the currently visible page
msgctxt "menu_refresh"
msgid "Refresh"
msgstr "ລີເຟຣສ໌"

#. Menu item: Share the currently visible page
msgctxt "menu_share"
msgid "Share"
msgstr "ແບ່ງປັນ"

#. Menu item: Open the currently visible page in another browser
msgctxt "menu_open_with"
msgid "Open with"
msgstr "ເປີດດ້ວຍ"

msgctxt "menu_settings"
msgid "Settings"
msgstr "ການຕັ້ງຄ່າ"

msgctxt "menu_about"
msgid "About"
msgstr "ກ່ຽວກັບ"

msgctxt "menu_help"
msgid "Help"
msgstr "ວິທີໃຊ້"

msgctxt "menu_rights"
msgid "Your Rights"
msgstr "ສິດທິຂອງທ່ານ"

msgctxt "preference_category_privacy"
msgid "Privacy"
msgstr "ຄວາມເປັນສ່ວນຕົວ"

msgctxt "preference_privacy_block_ads"
msgid "Block ad trackers"
msgstr "ບັອກຕົວຕິດຕາມຂອງການໂຄສະນາ"

msgctxt "preference_privacy_block_analytics"
msgid "Block analytic trackers"
msgstr "ບັອກຕົວຕິດຕາມຂອງການວິເຄາະ"

msgctxt "preference_privacy_block_social"
msgid "Block social trackers"
msgstr "ບັອກຕົວຕິດຕາມຂອງສືສັງຄົມອອນໄລນ໌"

msgctxt "preference_privacy_block_content"
msgid "Block other content trackers"
msgstr "ບັອກຕົວຕິດຕາມຂອງເນື້ອຫາຕ່າງໆ"

msgctxt "preference_privacy_block_content_summary"
msgid "May break some videos and Web pages"
msgstr "ອາດຈະເຮັດໃຫ້ບາງວິດີໂອ ແລະ ຫນ້າເວັບບໍ່ສົມບູນ"

msgctxt "preference_category_performance"
msgid "Performance"
msgstr "ປະສິດທິພາບ"

msgctxt "preference_performance_block_webfonts"
msgid "Block Web fonts"
msgstr "ບັອກຕົວອັກສອນເວັບ"

msgctxt "preference_category_mozilla"
msgid "Mozilla"
msgstr "Mozilla"

msgctxt "preference_mozilla_telemetry"
msgid "Send anonymous usage data"
msgstr "ສົ່ງຂໍ້ມູນການໃຊ້ງານແບບບໍ່ລະບຸຕົວຕົນ"

msgctxt "preference_mozilla_telemetry_summary"
msgid "Learn more"
msgstr "ຮຽນຮູ້ເພີ່ມເຕີມ"
