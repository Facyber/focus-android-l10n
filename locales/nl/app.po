#. This text is shown in the URL bar when it's empty
msgctxt "urlbar_hint"
msgid "Search or enter address"
msgstr "Voer zoekterm of adres in"

msgctxt "teaser"
msgid ""
"Automatic private browsing.\n"
"Browse. Erase. Repeat."
msgstr ""
"Automatische privénavigatie.\n"
"Surfen. Wissen. Herhalen."

#. "Snackbar" shown after the user has pressed the 'erase' button at the end
#. of a browsing session
msgctxt "feedback_erase"
msgid "Your browsing history has been erased."
msgstr "Uw navigatiegeschiedenis is gewist."

#. This label is shown below the URL bar as soon as the user enters text.
#. Clicking the label will perform a search
#, c-format
msgctxt "search_hint"
msgid "Search for %1$s"
msgstr "Zoeken naar %1$s"

#. Menu item: Share the currently visible page
msgctxt "menu_share"
msgid "Share…"
msgstr "Delen…"

#. Menu item: Switch to another browser to open the currently visible page.
#. %1$s will be       replaces with the name of the browser like: "Open with
#. Firefox" or "Open with Chrome". Some       links can be opened by native
#. apps that aren't a browser, e.g. "Open with YouTube"
#, c-format
msgctxt "menu_open_with_default_browser"
msgid "Open with %1$s"
msgstr "Openen met %1$s"

#. Menu item: Switch to another browser to open the currently visible page.
#. This menu item       will be shown if there are multiple browsers and we do
#. not know which one is the default       browser or in case Focus is actually
#. the default browser. Clicking this menu item will show       a screen with
#. multiple browsers (or other apps that can open the current URL) to choose
#. from.
msgctxt "menu_open_with_a_browser"
msgid "Open with…"
msgstr "Openen met…"

msgctxt "menu_settings"
msgid "Settings"
msgstr "Instellingen"

msgctxt "menu_about"
msgid "About"
msgstr "Over"

msgctxt "menu_help"
msgid "Help"
msgstr "Help"

msgctxt "menu_rights"
msgid "Your Rights"
msgstr "Uw rechten"

msgctxt "preference_category_privacy"
msgid "Privacy"
msgstr "Privacy"

msgctxt "preference_privacy_block_ads"
msgid "Block ad trackers"
msgstr "Advertentietrackers blokkeren"

msgctxt "preference_privacy_block_analytics"
msgid "Block analytic trackers"
msgstr "Analysetrackers blokkeren"

msgctxt "preference_privacy_block_social"
msgid "Block social trackers"
msgstr "Sociale trackers blokkeren"

msgctxt "preference_privacy_block_content"
msgid "Block other content trackers"
msgstr "Andere inhoudstrackers blokkeren"

msgctxt "preference_privacy_block_content_summary"
msgid "May break some videos and Web pages"
msgstr "Kan sommige video’s en webpagina’s verstoren"

msgctxt "preference_category_performance"
msgid "Performance"
msgstr "Prestaties"

msgctxt "preference_performance_block_webfonts"
msgid "Block Web fonts"
msgstr "Weblettertypen blokkeren"

msgctxt "preference_performance_block_images"
msgid "Block Images"
msgstr "Afbeeldingen blokkeren"

#. This preference does not set Focus as the default browser but instead links
#. to Android's      "default apps" screen or if not supported by this Android
#. version to a SUMO page describing      how to set Focus as a default browser
msgctxt "preference_default_browser"
msgid "Make default browser"
msgstr "Standaardbrowser maken"

msgctxt "preference_category_mozilla"
msgid "Mozilla"
msgstr "Mozilla"

msgctxt "preference_mozilla_telemetry"
msgid "Send anonymous usage data"
msgstr "Anonieme gebruiksgegevens verzenden"

msgctxt "preference_mozilla_telemetry_summary"
msgid "Learn more"
msgstr "Meer info"

#. Content description (not visible, for screen readers etc.): Clear text in
#. URL bar
msgctxt "content_description_clear_input"
msgid "Clear input"
msgstr "Invoer wissen"

#. Content description (not visible, for screen readers etc.): Clicking on the
#. website overlay      while in typing mode will dismiss and go back to the
#. browser. This text will be read when the      overlay if focused. See
#. https://github.com/mozilla-mobile/focus-android/issues/50
msgctxt "content_description_dismiss_input"
msgid "Dismiss"
msgstr "Verwijderen"

#. Content description (not visible, for screen readers etc.): Erase button:
#. Erase the browsing      history and go back to the home screen.
msgctxt "content_description_erase"
msgid "Erase browsing history"
msgstr "Navigatiegeschiedenis wissen"

#. Content description (not visible, for screen readers etc.): Lock icon
#. showed in the URL bar for      https pages..
msgctxt "content_description_lock"
msgid "Secure connection"
msgstr "Beveiligde verbinding"

#. This text is not visible. Instead it will be read by the accessibility
#. service once we start      loading a website
msgctxt "accessibility_announcement_loading"
msgid "Loading"
msgstr "Laden"

#. This text is not visible. Instead it will be read by the accessibility
#. service once we      finished loading a website
msgctxt "accessibility_announcement_loading_finished"
msgid "Website loaded"
msgstr "Website geladen"

#. Content description (not visible, for screen readers etc.): "Three dot"
#. menu button.
msgctxt "content_description_menu"
msgid "More options"
msgstr "Meer opties"

#. Content description (not visible, for screen readers etc.): Navigate
#. forward (browsing history)
msgctxt "content_description_forward"
msgid "Navigate forward"
msgstr "Vooruit bladeren"

#. Content description (not visible, for screen readers etc.): Reload current
#. website
msgctxt "content_description_reload"
msgid "Reload website"
msgstr "Website opnieuw laden"

msgctxt "firstrun_title"
msgid "Browse like no one’s watching."
msgstr "Surf alsof niemand meekijkt."

msgctxt "firstrun_description"
msgid "Automatically block online trackers while you browse. Then tap to erase visited pages, searches, cookies and passwords from your device."
msgstr "Blokkeer automatisch onlinetrackers terwijl u surft. Tik daarna om bezochte pagina’s, zoekopdrachten, cookies en wachtwoorden op uw apparaat te wissen."

msgctxt "firstrun_button"
msgid "Ok, got it!"
msgstr "OK, begrepen!"

msgctxt "firstrun_footer"
msgid "To be clear, sites and service you use can still know where you've been."
msgstr "Voor de duidelijkheid: websites en services die u gebruikt, kunnen nog steeds zien waar u bent geweest."
